{
    "name": "minulislam/packager",
    "description": "Laravel Package Skeleton Generator",
    "keywords": ["laravel","package", "generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Console\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/filesystem": "^5.5|^5.6|^5.7",
        "illuminate/support": "^5.5|^5.6|^5.7",
        "symfony/console": "~4.0",
        "symfony/filesystem": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/packager"
    ]
}
